{
  "title": "airdb",
  "description": "gitbook",
  "language": "en-us",
  "plugins": [
    "expandable-chapters",
    "anchors",
    "github",
    "editlink",
    "custom-favicon",
    "prism", "-highlight",
    "-lunr", "-search", "-search-pro", "search-plus",
    "codesnippet",
    "splitter",
    "page-toc-button",
    "image-captions",
    "page-footer-ex",
    "tags",
    "multipart",
    "theme-comscore",
    "gittalk"
  ],
  "pdf": {
    "toc": true,
    "pageNumbers": true,
    "fontSize": 11
  },
  "pluginsConfig": {
    "favicon": "favicon.ico",
    "image-captions": {
          "caption": "image - _CAPTION_"
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
   	},
    "expandable-chapters":{
    },
    "github": {
      "url": "https://github.com/airdb/gitbook"
    },
    "editlink": {
      "base": "https://github.com/airdb/gitbook/blob/master/",
      "label": "Edit This Page"
    },
    "page-footer-ex":{
        "copyright":"Copyright Â© 2015-present airdb.",
        "update_label":"LastUpdated: ",
        "update_format":"YYYY-MM-DD HH:mm:ss"
    },
    "tags": {
        "placement": "bottom"
    },
    "gittalk": {
      "clientID": "080237dd43387e94dd3c",
      "clientSecret": "35bb318402a5949a31c02518b0569255f7e2d92a",
      "repo": "airdb-wiki/comments",
      "owner": "deancn",
      "admin": ["deancn"],
      "distractionFreeMode": false 
    }
  }
}
